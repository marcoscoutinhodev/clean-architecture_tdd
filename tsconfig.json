{
  "compilerOptions": {
    "allowJs": true,
    "target": "es2019",
    "module": "commonjs",
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "strict": true,
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"]
    }
  },
  "include": [
    "src",
    "globalConfig.json"
  ],
  "exclude": []
}
